{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "ESNext"
    ],
    "rootDir": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "strict": true,
    "skipLibCheck": true
  }
}
